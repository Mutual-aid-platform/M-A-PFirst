/**index.wxss**/
.title{
  position: relative;
  display: flex;
  height: 35px;
 box-shadow: 0 0 5px black;
}
.photo image{
  width: 30px;
  height: 30px;
}
.list image{
  padding-right: 120px;
  width: 40px;
  height: 30px;
}
.listcontent{
  display: block;
  position: absolute;
  background-color: white;
  box-shadow: 0 0 5px black;
  width: 120px;
  height: 200px;
  top: 5%;
}
.listcontent view{
  height: 84rpx;
  line-height: 45px;
  border-bottom: 4rpx inset rgb(138, 137, 137);
}
 .titleName{
   width: 100px;
   position: relative;
   left: -25px;
   margin: 3px 30px;
   font-size: 16px;
   color: rgb(78, 87, 87);
 }

 .swiper-container{
   position: relative;
  width: 100%;
  height: 400rpx;
}
.item{
  height: 150px;
}
.item image{
  width: 100%;
  height: 150%;
}
 .functionBox{
   display: flex;
   flex-wrap: wrap;
   margin:10% 3%;
   width: 94%;
   height: 400px;
   border-radius: 10%;
   box-shadow: 0 0 1px black;
  }

.functionButton{
  margin-left: 20px;
  width: 310rpx;
  height: 310rpx;
 text-align: center;
 font-size: 20px;
 line-height: 250px;
 color: rgb(0, 0, 0);
 /* border-radius: 10%; */
 /* box-shadow: 0 0 5px black; */
}
.functionButton image{
  margin: 15%;
  margin-top: 5%;
  margin-bottom: 5%;
  width: 130px;
  height: 130px;
}
.functionButton text{
  position: relative;
  top: -420rpx;
}
.versign_box{
  margin-top: 80rpx;
  display:flex;
  flex-direction: column;
  align-items: center;
  font-size: 9pt; 
  color: #7f7f7f;
}
